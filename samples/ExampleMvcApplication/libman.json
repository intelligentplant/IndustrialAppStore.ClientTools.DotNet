{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/**",
        "js/**"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "**"
      ]
    },
    {
      "library": "jquery-validate@1.19.4",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "**"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "**"
      ]
    },
    {
      "library": "Chart.js@3.7.1",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "**"
      ]
    },
    {
      "library": "chartjs-adapter-luxon@1.1.0",
      "destination": "wwwroot/lib/chartjs-adapter-luxon/",
      "files": [
        "**" 
      ]
    },
    {
      "library": "luxon@2.3.0",
      "destination": "wwwroot/lib/luxon/",
      "files": [
        "**" 
      ]
    },
    {
      "library": "rxjs@7.5.4",
      "destination": "wwwroot/lib/rxjs/",
      "files": [
        "**"
      ]
    },
    {
      "library": "font-awesome@6.0.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "sprites/brands.svg",
        "sprites/regular.svg",
        "sprites/solid.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2"
      ]
    }
  ]
}